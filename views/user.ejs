


<% include header.ejs %>


<div>
	<h1><%= title %></h1>
    <p>Welcome to page <%= page %></p>
	<br />
	<br />
    <table class="table">
    	<tr>
    		<th>Name</th>
    		<th>Surname</th>
    		<th>Username</th>
    		<th>Password</th>
    	</tr>
    <% for(var i=0; i<users.length; i++) {%>
	   <tr>
	   		<td><%= users[i].user_name %></td>
	   		<td><%= users[i].user_surname %></td>
	   		<td><%= users[i].user_username %></td>
	   		<td><%= users[i].user_password %></td>
	   </tr>
	<% } %>
	</table>
</div>


